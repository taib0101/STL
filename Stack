#include<bits/stdc++.h>
using namespace std;
int main()
{
    stack < int > st;
    st.push(2);
    st.push(5);
    st.push(4);
    st.push(10);
    st.push(434);//added by sadman
    st.push(100);//added by sadman
    cout << "size = " << st.size() << endl;
    while(!st.empty())
    {
        cout << st.top() << endl;
        st.pop();
    }
    return 0;
}
